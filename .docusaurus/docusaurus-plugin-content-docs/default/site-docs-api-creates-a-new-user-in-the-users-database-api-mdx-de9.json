                  "minLength": 8,
                  "pattern": "/(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])/",
                  "type": "string",
                  "description": "User password, MUST contain a mix of upper and lower case letters, as well as digits and special characters",
